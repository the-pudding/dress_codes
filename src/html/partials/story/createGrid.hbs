<div class='create__grid'>
  {{#each doc.createItem}}
  <div class='grid__item grid__item-{{@index}}' data-gen='{{{gen}}}' data-race='{{{race}}}'>
      <p class='grid__item-name tk-atlas'>{{{name}}}</p>

      <!-- <img class='grid__item-img' src='assets/images/{{slug}}.png'> -->
      <div class='grid__item-icons'>
        <img class='grid__item-img' src='assets/images/placeholder.png'>
        @@include('x-square.svg')
      </div>


  </div>
  {{/each}}
</div>
