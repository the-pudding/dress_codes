<section class='words'>
  <h3 class='title__section'>{{doc.wordsTitle}}</h3>
  {{#each doc.wordsIntro}}
    <p class='prose'>{{{value}}}</p>
  {{/each}}

  {{> story/wordExamples }}

  {{#each doc.wordsMid}}
    <p class='prose'>{{{value}}}</p>
  {{/each}}

  <figure class='words__explore'>
    <h4 class='chart__title'>{{{doc.wordsGraphTitle}}}</h4>
    <p class='chart__subtitle'>{{{doc.wordsGraphSub}}}</p>
    <img class='words__img-examples' src='assets/images/placeholder_words.png'>
  </figure>


</section>
